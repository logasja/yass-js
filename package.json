{
  "name": "yass-js",
  "version": "1.1.3",
  "description": "A steganographic encoder and decoder built on top of a jpeg library.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "minify": "uglifyjs --compress --mangle --output name.min.js",
    "test": "mocha -r ts-node/register test/*.spec.ts",
    "test:jpeg": "mocha -r ts-node/register test/jpeg.spec.ts",
    "test:qim": "mocha -r ts-node/register test/qim.spec.ts",
    "test:repeat_accumulation": "mocha -r ts-node/register test/repeat_accumulation.spec.ts",
    "test:steg": "mocha -r ts-node/register test/steg.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/spudlab/yass-js"
  },
  "keywords": [
    "jpeg",
    "jpg",
    "encoder",
    "decoder",
    "codec",
    "image",
    "javascript",
    "js",
    "yass",
    "steganography"
  ],
  "author": "Jacob Logas <logasja@gatech.edu>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://trello.com/b/OE4fWKJW/stegoshare"
  },
  "dependencies": {
    "seededshuffle": "^0.2.0",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.24",
    "@types/seededshuffle": "^0.2.0",
    "chai": "^4.2.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.2.11",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "uglify-js": "^3.10.0",
    "xdne-js": "^1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ],
    "*.js": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true
  }
}
